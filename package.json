{
  "name": "nextjs-study",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rmdir /q/s .next; next dev --port 3000",
    "build": "rmdir /q/s build; next build",
    "export": "rmdir /q/s build; next build && next export",
    "start": "rmdir /q/s build; next build && next start --port 4000",
    "analyze": "rmdir /q/s build; cross-env ANALYZE=true yarn build"
  },
  "dependencies": {
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "axios": "0.21.1",
    "axios-extensions": "^3.1.3",
    "axios-mock-adapter": "1.19.0",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.9",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "node-sass": "5.0.0",
    "typescript": "^4.2.2"
  }
}